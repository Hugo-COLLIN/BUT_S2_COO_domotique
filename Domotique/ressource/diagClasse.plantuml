@startuml
class Lampe{
    - nom : String
    - allume : boolean
    + Lampe(String nom)
    + allumer()
    + eteindre()
    + toString()

}

class Telecommande{
    + Telecommande()
    + ajouterLampe(Lampe lampe)
    + activerLampe(int indexLampe)
    + desactiverLampe(int indexLampe)
    + activerTout()
    + toString()
}

class Hifi{
    - son : int
    + allumer()
    + eteindre()
    + toString() : String
}

Telecommande -> "*" Lampe : -lampes
@enduml